{
  "name": "jetpack",
  "version": "1.0.0",
  "description": "Webpack + Coffeescript + Stylus + React Boilerplate",
  "main": "bin/www",
  "scripts": {
    "start": "bin/www # localhost:3000",
    "dev": "(webpack-dev-server & nodemon) # localhost:3000",
    "build": "rm -rf dist/* && webpack",
    "analyze": "webpack",
    "test": "webpack && mocha test/main.js \"server/**/*.spec.*\"",
    "test:watch": "(webpack --watch >/dev/null & mocha --watch --reporter min test/main.js \"server/**/*.spec.*\")",
    "test:browser": "webpack-dev-server # localhost:8080"
  },
